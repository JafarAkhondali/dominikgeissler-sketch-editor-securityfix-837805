{
  "name": "sketch-editor",
  "version": "1.0.0",
  "dependencies": {
    "formidable": "^3.5.1",
    "tar": "^6.2.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "jsdom": "^23.0.1",
    "supertest": "^6.3.3"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "node src/lib/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "doc": "bash ./node_modules/.bin/jsdoc -r src/lib -d doc/dev/generated"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix .",
      "git add"
    ]
  }
}